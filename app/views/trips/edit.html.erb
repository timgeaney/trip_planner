<h2>Edit a new Trip</h2>
<div class="row">
  <div class="span6 offset3">
	<%= form_for @trip, html: { multipart: true, class: 'standard' } do |f| %>
	  <%= render 'shared/error_messages', object: f.object %>
	  <%= render :partial => 'form', locals: { f: f } %>

	  <div>
	    <%= f.submit "Edit your trip" %>
	  </div>
	<% end %>
	<div>
</div>